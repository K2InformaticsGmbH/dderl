{
  "name": "dderl",
  "version": "2.3.1",
  "description": "Web based tool for exploring databases",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/K2InformaticsGmbH/dderl.git"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "d3": "5.5.0",
    "d3-selection-multi": "1.0.1",
    "font-awesome": "4.7.0",
    "jquery": "^3.4.1",
    "jquery-dialogextend": "git+https://github.com/K2InformaticsGmbH/jquery-dialogextend.git#585a7e2d59925481b32f4e7800d49aeced3a3d35",
    "jquery-ui": "1.12.1",
    "monaco-editor": "0.14.3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "css-loader": "0.28.11",
    "extract-loader": "1.0.2",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "imports-loader": "0.7.1",
    "jasmine-core": "2.9.1",
    "jshint": "2.9.6",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.5",
    "rimraf": "2.6.2",
    "style-loader": "0.19.1",
    "webpack": "3.10.0"
  },
  "scripts": {
    "install-build-prod": "yarn install && yarn build-prod",
    "build": "yarn build-dev",
    "build-dev": "yarn lint && yarn clean && webpack --progress",
    "build-prod": "yarn lint && yarn clean && webpack -p",
    "clean": "rimraf ../public/dist",
    "env": "env",
    "lint": "jshint --verbose static/index.js static/scripts static/dashboard static/dialogs static/graph static/slickgrid",
    "test": "karma start karma.conf.js",
    "watch": "yarn clean && webpack --progress --watch",
    "lint:d3_examples": "jshint --verbose d3_examples"
  }
}
